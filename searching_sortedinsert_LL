#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
struct node{
    int element;
    struct node*link;
}
int size=0;
struct node * pos_traverser(struct node * head,int pos){
    struct node * ptr=head;
    while(ptr!=NULL && pos >0){
        ptr=ptr->link;
        pos--;
    }
    return ptr;
}
void search(struct node * head,int data){
    int count=0;
    struct node *ptr=head;
    while(ptr!=NULL && ptr->element!=data){
        ptr=ptr->link;
        count++;
    }
}
